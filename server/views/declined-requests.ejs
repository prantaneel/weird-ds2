<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Admin Approve</title>
    <link rel="stylesheet" href="./styles/admin-approve.css"></link>
    <link rel="stylesheet" href="/styles/_global.css">
    <!--Inter UI font-->
    <link href="https://rsms.me/inter/inter-ui.css" rel="stylesheet" />

    <!--vendors styles-->
    <link
      rel="stylesheet"
      href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css"
    />

    <!-- Bootstrap CSS / Color Scheme -->
    <link rel="stylesheet" href="/knight/css/default.css" id="theme-color" />
    <style>
      .navbar {
        backdrop-filter: saturate(180%) blur(10px);
        background-image: linear-gradient(
          rgba(0, 0, 0, 0.637),
          rgba(0, 0, 0, 0)
        );
      }
    </style>
  </head>
  <body>
    <input id="page-nav-toggle" class="main-navigation-toggle" type="checkbox" />
    <label for="page-nav-toggle">
      <svg class="icon--menu-toggle" viewBox="0 0 60 30">
        <g class="icon-group">
          <g class="icon--menu">
            <path d="M 10 5 L 70 5" />
            <path d="M 10 20 L 70 20" />
            <path d="M 10 35 L 70 35" />
          </g>
          <g class="icon--close">
            <path d="M 15 0 L 45 30" />
            <path d="M 15 30 L 45 0" />
          </g>
        </g>
      </svg>
    </label>
    <nav class="main-navigation">
      <ul>
        <li><a href="/">Home</a></li>
        <li><a href="/blogs">Blogs</a></li>
        <li><a href="/blog-editor">Create</a></li>
        <li><a href="#" id="logout">Logout</a></li>
        <li><a href="/admin-register">Register</a></li>
        <li><a href="/admin-password-change">Change Password</a></li>


      </ul>
    </nav>
    <form method="post" action="/admin-logout?_method=DELETE" id="logout-form"></form>

    <div class="request-head"><h1>Declined Requests</h1></div>
    <table id = "data-table">
      <tr class="row-labels">
        <td>Blog Id</td>
        <td>Blog Title</td>
        <td>Publish Time</td>
      </tr>
    </table>
    <div id="empty-div"></div>
    <div class="request-head-decline">[<a href="/admin-approve">Pending Requests</a>]</div>
    <script>
      var declined_admin_requests = <%-JSON.stringify(declined_requests)%>;
      //console.log(declined_admin_requests);
    </script>
     <!--footer-->
     <footer class="py-6">
      <hr style="width: 80%; margin: 30px auto" />

       <div class="container">
         <div class="row">
           <div class="col-sm-5 mr-auto">
             <h5>About Algo Blog</h5>
             <p class="text-muted">
               A platform developed as a side project for publishing articles on
               computer science algortihms.
             </p>
             <ul class="list-inline social social-sm">
               <li class="list-inline-item">
                 <a href="https://www.instagram.com/pranta.neel/"
                   ><i class="fa fa-instagram"></i
                 ></a>
               </li>
               <li class="list-inline-item">
                 <a href="https://www.facebook.com/prantaneel.debnath.18"
                   ><i class="fa fa-facebook"></i
                 ></a>
               </li>
               <li class="list-inline-item">
                 <a href="#"><i class="fa fa-dribbble"></i></a>
               </li>
             </ul>
           </div>
         </div>
       </div>
     </footer>
    <script src="https://code.jquery.com/jquery-3.2.1.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.12.9/umd/popper.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/js/bootstrap.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/feather-icons/4.7.3/feather.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/slick-carousel/1.8.1/slick.min.js"></script>
    <script src="js/scripts.js"></script>
  </body>
  <script src="./scripts/adminapprove-decline.js"></script>
